<%= form_for @district do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :country %><br />
    <%= f.text_field :country %>
  </p>
  <p>
    <%= f.label :museum_address %><br />
    <%= f.text_field :museum_address %>
  </p>
  <p><%= f.submit %></p>
<% end %>
